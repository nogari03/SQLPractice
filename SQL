SELECT VEND_NAME, VEND_ADDRESS, VEND_COUNTRY
FROM VENDORS
ORDER BY VEND_NAME ASC, VEND_COUNTRY DESC;

SELECT PROD_ID, VEND_ID, PROD_NAME, PROD_PRICE
FROM PRODUCTS
ORDER BY PROD_PRICE DESC;

SELECT CUST_NAME, CUST_ADDRESS, CUST_COUNTRY
FROM CUSTOMERS
ORDER BY CUST_NAME DESC;

SELECT ORDER_NUM, ORDER_DATE, CUST_ID
FROM ORDERS
ORDER BY ORDER_DATE DESC,
         CUST_ID ASC;

SELECT CUST_NAME
FROM CUSTOMERS
WHERE CUST_EMAIL IS NULL
ORDER BY CUST_NAME DESC;

SELECT CUST_NAME, CUST_ADDRESS
FROM CUSTOMERS
WHERE CUST_CITY = 'Chicago';

SELECT PROD_ID,PROD_NAME
FROM PRODUCTS
WHERE PROD_PRICE > 5
ORDER BY PROD_NAME ASC;

SELECT VEND_NAME, VEND_ADDRESS
FROM VENDORS
WHERE VEND_COUNTRY = 'France'
ORDER BY VEND_NAME DESC;

SELECT ORDER_NUM, PROD_ID, ITEM_PRICE
FROM ORDERITEMS
WHERE ITEM_PRICE < 10
ORDER BY ITEM_PRICE ASC;

SELECT CUST_NAME
FROM CUSTOMERS
WHERE CUST_CONTACT = 'Kim Howard' AND CUST_EMAIL IS NULL;

SELECT PROD_NAME
FROM PRODUCTS
WHERE VEND_ID = 'DLL01' AND PROD_PRICE <= 5
ORDER BY PROD_NAME ASC;

SELECT VEND_NAME, VEND_ADDRESS, VEND_ZIP
FROM VENDORS
WHERE VEND_CITY = 'New York'
ORDER BY VEND_NAME DESC;

SELECT ORDER_NUM
FROM ORDERITEMS
WHERE (PROD_ID = 'BNBG01' OR PROD_ID = 'BR03');

SELECT ORDER_NUM
FROM ORDERITEMS
WHERE ITEM_PRICE BETWEEN 5 AND 9
ORDER BY ORDER_NUM ASC;

SELECT PROD_NAME
FROM PRODUCTS
WHERE VEND_ID <> 'DLL01'
ORDER BY PROD_NAME DESC;

SELECT CUST_NAME
FROM CUSTOMERS
WHERE CUST_NAME IN ('Kids Place','Village Toys')
AND CUST_CITY <> 'Detroit';

SELECT VEND_NAME
FROM VENDORS
WHERE VEND_NAME IN ( 'Bear Emporium','Jouets et ours')
AND VEND_STATE IS NULL;

SELECT ORDER_NUM
FROM ORDERITEMS
WHERE PROD_ID IN ('BR03','BNBG01')
AND QUANTITY >= 100
ORDER BY ORDER_NUM ASC;

SELECT ORDER_NUM
FROM ORDERITEMS
WHERE PROD_ID NOT IN ('BR03','BNBG01')
AND QUANTITY >= 50
ORDER BY ORDER_NUM DESC;

SELECT CUST_NAME, CUST_ADDRESS
FROM CUSTOMERS
WHERE CUST_NAME LIKE 'Fish%'
AND CUST_CITY LIKE 'P%'
ORDER BY CUST_NAME DESC;

SELECT PROD_ID, VEND_ID, PROD_PRICE
FROM PRODUCTS
WHERE TRIM(PROD_ID) LIKE 'B%1'
ORDER BY PROD_PRICE ASC;

SELECT VEND_NAME, VEND_ADDRESS
FROM VENDORS
WHERE VEND_ID LIKE 'D%'
AND VEND_CITY LIKE 'D%'
ORDER BY VEND_NAME ASC;

SELECT ORDER_NUM, ITEM_PRICE
FROM ORDERITEMS
WHERE PROD_ID LIKE '%BG%'
AND QUANTITY = 100
ORDER BY ITEM_PRICE ASC;

SELECT TRIM(CUST_NAME)||'/'||TRIM(CUST_ADDRESS)
FROM CUSTOMERS
WHERE CUST_ADDRESS LIKE '%south%';

SELECT SUBSTR(PROD_ID,0,2)
FROM ORDERITEMS
WHERE ORDER_NUM NOT IN (20005, 20007);

SELECT CUST_NAME, REPLACE(CUST_ADDRESS,'Drive','Car')
FROM CUSTOMERS;

SELECT VEND_NAME, SUBSTR(VEND_ZIP,0,2)
FROM VENDORS
WHERE VEND_ID LIKE 'BR%'
ORDER BY VEND_NAME DESC;

SELECT VEND_NAME, DECODE(SUBSTR(VEND_ZIP,0,2),'44','A','99','B','11','C','D')
FROM VENDORS
WHERE VEND_ID <> 'BRE02'
ORDER BY VEND_NAME DESC;

SELECT CUST_NAME, COALESCE(CUST_EMAIL,CUST_ADDRESS)
FROM CUSTOMERS;

SELECT REPLACE(PROD_NAME,'bear','toy')
FROM PRODUCTS
WHERE PROD_NAME LIKE '%bear%';

SELECT PROD_NAME, ROUND(PROD_PRICE)
FROM PRODUCTS
WHERE PROD_ID LIKE 'BR%';

SELECT ORDER_NUM, PROD_ID, FLOOR(ITEM_PRICE)
FROM ORDERITEMS;
